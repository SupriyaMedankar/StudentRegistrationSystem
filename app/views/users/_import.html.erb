<div class="flex flex-row">
  <%= form_tag import_users_path, method: :post, multipart: true do %>
    <%= file_field_tag :file, accept: 'csv', class: "rounded-lg py-3 px-5 block font-medium inline-block" %>
    <%= submit_tag 'Import', class: "rounded-lg py-3 px-5 bg-blue-600 text-white block font-medium inline-block" %>
  <% end %>
</div>
